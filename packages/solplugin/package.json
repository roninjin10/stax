{
  "name": "@stax/solplugin",
  "version": "0.0.0",
  "description": "TypeScript Language Service Plugin for Solidity",
  "keywords": [
    "typescript",
    "solidity",
    "language service"
  ],
  "main": "build/index.js",
  "bin": {
    "tsgql": "build/cli/cli.js",
    "ts-graphql-plugin": "build/cli/cli.js"
  },
  "typings": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf build \"e2e/*.log\" \"*.tsbuildinfo\"",
    "build": "tsc -p .",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "jest": "jest",
    "jest:ci": "jest --coverage --maxWorkers=4",
    "e2e": "node e2e/run.js",
    "e2e:ci": "c8 -o e2e_coverage -x e2e -r json -i \"src/**/*\" node e2e/run.js",
    "nx:pretest": "pnpm build",
    "test": "pnpm format:check && pnpm lint && pnpm jest:ci",
    "prettier": "prettier \"*.js\" \"*.md\" \"*.json\" \"*.yml\" \"src/**/*\" \"e2e/**/*\" \"docs/**/*\"",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "doc:toc": "ts-node -P tools/tsconfig.json tools/add-toc.ts",
    "watch:compile": "tsc --watch -p .",
    "watch:jest": "jest --watch",
    "watch": "pnpm run clean  && run-p watch:*",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "author": "Quramy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quramy/ts-graphql-plugin.git"
  },
  "dependencies": {
    "graphql-language-service-interface": "^2.10.2",
    "graphql-language-service-types": "^1.8.7"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/jest": "28.1.8",
    "@types/node": "18.11.9",
    "@types/node-fetch": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "c8": "7.12.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "express": "4.18.2",
    "express-graphql": "0.12.0",
    "fretted-strings": "1.0.1",
    "glob": "8.0.3",
    "graphql": "15.5.0",
    "graphql-config": "4.3.6",
    "jest": "28.1.3",
    "markdown-toc": "1.2.0",
    "node-fetch": "3.3.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.8.0",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "talt": "2.4.0",
    "ts-jest": "28.0.8",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "typescript-eslint-language-service": "5.0.0",
    "vscode-languageserver-types": "3.17.2",
    "webpack": "5.74.0",
    "webpack-cli": "5.0.0"
  },
  "resolutions": {
    "@types/express-serve-static-core": "^4.17.9"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0",
    "typescript": "^2.3.0 || ^3.0.0 || ^4.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "build/.*"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/testing/**"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
